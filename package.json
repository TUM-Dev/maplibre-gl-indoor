{
  "name": "mapbox-gl-indoor",
  "description": "Visualize multi-level buildings",
  "version": "0.0.6",
  "main": "dist/mapbox-gl-indoor.js",
  "module": "dist/mapbox-gl-indoor.es.js",
  "files": [
    "dist"
  ],
  "private": false,
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "scripts": {
    "build": "rollup -c",
    "build-token": "node build/generate-access-token-script.js",
    "lint": "eslint --ext .js,.ts src",
    "start-server": "st --no-cache -H 0.0.0.0 --port 9966 .",
    "start": "NODE_ENV=debug run-p build-token watch start-server",
    "watch": "rollup -c --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox-gl-indoor/mapbox-gl-indoor.git"
  },
  "keywords": [
    "indoor",
    "building",
    "multi-level",
    "floors",
    "geojson",
    "mapbox",
    "mapboxgl",
    "ui"
  ],
  "author": "Thibaud Michel",
  "license": "MIT",
  "peerDependencies": {
    "mapbox-gl": ">1.10.0"
  },
  "devDependencies": {
    "@mapbox/geojson-types": "^1.0.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^3.1.1",
    "@types/geojson": "^7946.0.7",
    "@types/mapbox-gl": "^1.9.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "brfs": "^2.0.2",
    "browserify": "^16.5.1",
    "budo": "^11.6.3",
    "eslint": "^6.8.0",
    "insert-css": "^2.0.0",
    "mapbox-gl": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "osmtogeojson": "^3.0.0-beta.4",
    "rollup": "^1.20.0",
    "rollup-plugin-terser": "^7.0.0",
    "st": "^2.0.0",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@turf/bbox": "^6.0.1",
    "@turf/distance": "^6.0.1"
  }
}
